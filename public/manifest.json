{
  "name": "SHU排课助手",
  "version": "0.2.3",
  "description": "上海大学排课助手(插件版)",
  "homepage_url": "https://xk.zkllab.com",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.shu.edu.cn/StudentQuery/QueryCourse"
      ],
      "css": [
        "csCommon.css"
      ],
      "js": [
        "csQueryCourse.js"
      ]
    },
    {
      "matches": [
        "http://*.shu.edu.cn/CourseSelectionStudent/FastInput"
      ],
      "css": [
        "csCommon.css"
      ],
      "js": [
        "csFastInput.js"
      ]
    },
    {
      "matches": [
        "http://*.shu.edu.cn/StudentQuery/QueryCourseTable"
      ],
      "css": [
        "csCommon.css"
      ],
      "js": [
        "csQueryCourseTable.js"
      ]
    },
    {
      "matches": [
        "https://xk.zkllab.com/*"
      ],
      "js": [
        "js/chunk-vendors.js",
        "js/app.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}